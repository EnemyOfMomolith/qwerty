<Window x:Class="чзхноваялаба.ChangePrior"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:чзхноваялаба"
        mc:Ignorable="d"
        Title="ChangePrior" Height="300" Width="400">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>                       
        </Grid.RowDefinitions>
        <TextBlock Text="Текущик приоритет" VerticalAlignment="Center" HorizontalAlignment="Center"/>
        <Button Content="Сохранить приоритет" Grid.Row="1" VerticalAlignment="Center" FontSize="16" HorizontalAlignment="Center"/>
    </Grid>
</Window>
